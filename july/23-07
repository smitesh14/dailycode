class Solution {
public:
    int fib(int n) {
        switch(n){
            case 0:
            return 0;
            break;
            case 1:
            return 1;
            break;
            default:
            return (fib(n-1)+fib(n-2));
        }
    }
    int main(){
    int m;
    cin>>m;
    cout<<fib(m);
    return 0;
 system("pause>0");
    
}
};
